    "postcss-prune-var": "^1.0.4",
    "prettier": "2.8.8",
    "tailwindcss": "^3.3.2",
    "tsup": "6.7.0",
    "typescript": "5.0.4"
  },
  "peerDependencies": {
    "tailwindcss": ">=3.3.0"
  }
